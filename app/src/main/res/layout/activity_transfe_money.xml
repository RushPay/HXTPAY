<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_transfe_money"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hxtao.qmd.hxtpay.activity.TransfeMoneyActivity">
    <Button
        android:id="@+id/transfer_money_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:visibility="gone"
        android:text="转账"/>
    <ListView
        android:id="@+id/list_transfr_money_act"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/transfer_money_btn">
    </ListView>

</RelativeLayout>
